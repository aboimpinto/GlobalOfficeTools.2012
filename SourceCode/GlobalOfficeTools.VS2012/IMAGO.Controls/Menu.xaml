                <ItemsControl.ItemTemplate>
                    <DataTemplate>
                        <TextBlock Text="xxx" />
                    </DataTemplate>
                </ItemsControl.ItemTemplate>
            </ItemsControl>-->
        </Border>
        
    </Grid>
</UserControl>
